{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "designer": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@ngrx/schematics:component": {
          "styleext": "scss"
        }
      },
      "root": "projects/designer",
      "sourceRoot": "projects/designer/src",
      "prefix": "dsgnr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/designer",
            "index": "projects/designer/src/index.html",
            "main": "projects/designer/src/main.ts",
            "polyfills": "projects/designer/src/polyfills.ts",
            "tsConfig": "projects/designer/tsconfig.app.json",
            "aot": false,
            "assets": ["projects/designer/src/favicon.ico", "projects/designer/src/assets"],
            "styles": ["projects/designer/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/designer/src/environments/environment.ts",
                  "with": "projects/designer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "projects/designer/src/environments/environment.ts",
                  "with": "projects/designer/src/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "designer:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "designer:build:production"
            },
            "hmr": {
              "hmr": true,
              "browserTarget": "designer:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "designer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/designer/src/test.ts",
            "polyfills": "projects/designer/src/polyfills.ts",
            "tsConfig": "projects/designer/tsconfig.spec.json",
            "karmaConfig": "projects/designer/karma.conf.js",
            "assets": ["projects/designer/src/favicon.ico", "projects/designer/src/assets"],
            "styles": ["projects/designer/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/designer/tsconfig.app.json",
              "projects/designer/tsconfig.spec.json",
              "projects/designer/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/designer/e2e/protractor.conf.js",
            "devServerTarget": "designer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "designer:serve:production"
            }
          }
        }
      }
    },
    "scheduler": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/scheduler",
      "sourceRoot": "projects/scheduler/src",
      "prefix": "schdlr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/scheduler",
            "index": "projects/scheduler/src/index.html",
            "main": "projects/scheduler/src/main.ts",
            "polyfills": "projects/scheduler/src/polyfills.ts",
            "tsConfig": "projects/scheduler/tsconfig.app.json",
            "aot": false,
            "assets": ["projects/scheduler/src/favicon.ico", "projects/scheduler/src/assets"],
            "styles": ["projects/scheduler/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/scheduler/src/environments/environment.ts",
                  "with": "projects/scheduler/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "projects/scheduler/src/environments/environment.ts",
                  "with": "projects/scheduler/src/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "scheduler:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "scheduler:build:production"
            },
            "hmr": {
              "hmr": true,
              "browserTarget": "scheduler:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "scheduler:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/scheduler/src/test.ts",
            "polyfills": "projects/scheduler/src/polyfills.ts",
            "tsConfig": "projects/scheduler/tsconfig.spec.json",
            "karmaConfig": "projects/scheduler/karma.conf.js",
            "assets": ["projects/scheduler/src/favicon.ico", "projects/scheduler/src/assets"],
            "styles": ["projects/scheduler/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/scheduler/tsconfig.app.json",
              "projects/scheduler/tsconfig.spec.json",
              "projects/scheduler/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/scheduler/e2e/protractor.conf.js",
            "devServerTarget": "scheduler:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "scheduler:serve:production"
            }
          }
        }
      }
    },
    "tadm-common": {
      "projectType": "library",
      "root": "projects/tadm-common",
      "sourceRoot": "projects/tadm-common/src",
      "prefix": "tadm-common",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/tadm-common/tsconfig.lib.json",
            "project": "projects/tadm-common/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/tadm-common/src/test.ts",
            "tsConfig": "projects/tadm-common/tsconfig.spec.json",
            "karmaConfig": "projects/tadm-common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/tadm-common/tsconfig.lib.json",
              "projects/tadm-common/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "designer"
}
